{
  "name": "chronical",
  "version": "0.1.2",
  "description": "An implementation of RRule as defined in iCalendar (RFC 5545) using Temporal API written in Rust",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/himura467/chronical.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "artifacts": "napi artifacts",
    "build": "pnpm build:rust && pnpm build:ts",
    "build:rust": "napi build --no-const-enum --platform --release --esm --output-dir src-js/generated",
    "build:rust:debug": "napi build --no-const-enum --platform --esm --output-dir src-js/generated",
    "build:ts": "tsgo",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version"
  },
  "keywords": [
    "icalendar",
    "rrule",
    "temporal",
    "rust",
    "rs"
  ],
  "author": {
    "name": "Akito Shitara",
    "email": "mitarashidango0927@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.18.0",
  "dependencies": {
    "temporal-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.5.0",
    "@typescript/native-preview": "7.0.0-dev.20251223.1"
  },
  "files": [
    "dist"
  ],
  "napi": {
    "binaryName": "chronical",
    "targets": [
      "aarch64-apple-darwin",
      "aarch64-pc-windows-msvc",
      "aarch64-unknown-linux-gnu",
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu"
    ]
  },
  "engines": {
    "node": ">= 20"
  }
}
